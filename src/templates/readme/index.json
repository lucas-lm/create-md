{
  "name": "readme",
  "sections": ["header", "about", "features", "requirements", "contributing", "todo", "techs", "author", "license"],
  "defaultSections": ["header", "about", "contributing", "author", "license"],
  "props": {
    "header": [
      { "name": "github username", "default": "create_md.github_username"}, 
      { "name": "github repo name", "default": "create_md.github_repo"}, 
      { "name": "title", "default": "name"}, 
      { "name": "headline", "default": "description"},
      { "name": "banner url", "description": "banner or logo url"}
    ],
    "about": [
      {"name": "homepage", "default": "homepage"},
      {"name": "long description"}
    ],
    "features": [{ "name": "feats", "description": "List features (comma separated)" }],
    "contributing": [
      {"name":"github username", "default": "create_md.github_username"}, 
      {"name":"github repo name", "default": "create_md.github_repo"}
    ],
    "author": [
      {"name": "author name", "default": "author.name"}, 
      {"name":"author website", "default": "author.url"}, 
      {"name":"author email", "default": "author.email"}, 
      {"name":"github username", "default": "create_md.github_username"}, 
      {"name": "twitter username"}, {"name": "linkedin username"}
    ],
    "todo": [{ "name": "todos", "description": "Tasks to be done (comma separated)" }],
    "techs": [{ "name": "techs", "description": "Main technologies used in the project (comma separated)" }],
    "requirements": [{"name": "node version"}, {"name": "npm version"}],
    "license": [
      {"name": "license", "default": "license"}, 
      {"name": "author name", "default": "author.name"}
    ]
  }
}
